const mongoose=require("mongoose")
const postschema=new mongoose.Schema(
    {
        userId:{
            required:true,
            type:mongoose.Schema.Types.ObjectId,
            ref:"userlogin"
        },
        post:{
            required:true,
            type:String
        },
        PostedDate:{
            required:true,
            type:Date,default:Date.now
        }

    }
)
module.exports=mongoose.model("postCollection",postschema)